; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "MarginContainer",
"class": "GameScreen",
"language": "GDScript",
"path": "res://nodes/screens/screen.gd"
} ]
_global_script_class_icons={
"GameScreen": ""
}

[application]

config/name="Developing With Screens In Godot"
run/main_scene="res://nodes/screens/menu-screen.tscn"
config/icon="res://images/icon.png"

[autoload]

Constants="*res://nodes/globals/constants.tscn"
Screens="*res://nodes/globals/screens.tscn"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://resources/environment.tres"
